{
  "name": "remix-peer-video-call-demo",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy ./build/client",
    "dev": "run-p dev:*",
    "dev:remix": "remix vite:dev",
    "dev:worker": "wrangler dev --config=src/wrangler.toml src/worker.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.9.1",
    "@remix-run/cloudflare-pages": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@xstate/react": "4.0.0-beta.11",
    "clsx": "^2.1.1",
    "isbot": "^4.1.0",
    "miniflare": "^3.20240404.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^7.6.0",
    "tiny-invariant": "^1.3.3",
    "xstate": "5.0.0-beta.44",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@cloudflare/workers-types": "^4.20240405.0",
    "@remix-run/dev": "^2.9.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "eslint-plugin-format": "^0.1.1",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "remix-development-tools": "^4.1.6",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.48.0"
  }
}
